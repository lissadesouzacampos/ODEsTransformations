               ODEsTransformations`Private`t]; 
            ODEsTransformations`Private`\[Xi][
               Pattern[ODEsTransformations`Private`t, 
                Blank[]]] = FullSimplify[
               Integrate[
                Sqrt[
                 ODEsTransformations`Private`r[
                 ODEsTransformations`Private`t]], 
                ODEsTransformations`Private`t]]; 
            ODEsTransformations`Private`\[CapitalPhi][
               Pattern[ODEsTransformations`Private`t, 
                Blank[]]] = 
             FullSimplify[
              ODEsTransformations`Private`r[ODEsTransformations`Private`t]^(1/
                 4) E^((1/2) Integrate[
                   ODEsTransformations`Private`p[
                   ODEsTransformations`Private`t], 
                   ODEsTransformations`Private`t])]; 
            ODEsTransformations`Private`aux1 = FullSimplify[
               ReplaceAll[
                ReplaceAll[
                ODEsTransformations`Private`eq, ODEsTransformations`Private`y -> 
                 Function[
                  ODEsTransformations`Private`t, (1/
                    ODEsTransformations`Private`\[CapitalPhi][
                    ODEsTransformations`Private`t]) 
                   ODEsTransformations`Private`\[Eta][
                    ODEsTransformations`Private`\[Xi][
                    ODEsTransformations`Private`t]]]], 
                ODEsTransformations`Private`\[Xi][
                 ODEsTransformations`Private`t] -> 
                ODEsTransformations`Private`\[Xi]]]; 
            ODEsTransformations`Private`aux2 = 
             Coefficient[ODEsTransformations`Private`aux1, 
               Derivative[2][ODEsTransformations`Private`\[Eta]][
               ODEsTransformations`Private`\[Xi]]]; 
            ODEsTransformations`Private`LNFormEQ = Simplify[
               ReplaceAll[
               ODEsTransformations`Private`aux1/
                ODEsTransformations`Private`aux2, 
                ODEsTransformations`Private`t -> 
                ODEsTransformations`Private`x]]; 
            ODEsTransformations`Private`V = Simplify[
               ReplaceAll[
               Coefficient[
                 ODEsTransformations`Private`aux1/
                  ODEsTransformations`Private`aux2, 
                  ODEsTransformations`Private`\[Eta][
                  ODEsTransformations`Private`\[Xi]]] - 
                ODEsTransformations`Private`\[Lambda], 
                ODEsTransformations`Private`t -> 
                ODEsTransformations`Private`x]]; Return[{
               ODEsTransformations`PrintLNEquation[
               ODEsTransformations`Private`V, 
                ODEsTransformations`Private`\[Lambda], 
                ODEsTransformations`Private`\[Eta], 
                ODEsTransformations`Private`\[Xi]], 
               ODEsTransformations`Private`LNFormEQ, 
               ODEsTransformations`Private`V, 
               ODEsTransformations`Private`\[Xi][
               ODEsTransformations`Private`x], 
               ODEsTransformations`Private`\[CapitalPhi][
               ODEsTransformations`Private`x]}]; Null]; Null]; Null), 
       ODEsTransformations`LiouvilleNormalForm[
         Pattern[ODEsTransformations`Private`eq, 
          Blank[]], 
         Pattern[ODEsTransformations`Private`y, 
          Blank[]], 
         Pattern[ODEsTransformations`Private`x, 
          Blank[]], 
         Pattern[ODEsTransformations`Private`\[Lambda], 
          Blank[]], 
         Pattern[ODEsTransformations`Private`\[Eta], 
          Blank[]], 
         Pattern[ODEsTransformations`Private`\[Xi], 
          Blank[]]] :> 
       Module[{ODEsTransformations`Private`coef, 
          ODEsTransformations`Private`aux1, ODEsTransformations`Private`aux2},
          ODEsTransformations`Private`coef = 
          ODEsTransformations`CollectCoefficients[
           ODEsTransformations`Private`eq, ODEsTransformations`Private`y, 
            ODEsTransformations`Private`x]; 
         ODEsTransformations`Private`aux1 = Coefficient[
            Part[ODEsTransformations`Private`coef, 3], 
            ODEsTransformations`Private`\[Lambda]]; 
         ODEsTransformations`Private`aux2 = 
          Part[ODEsTransformations`Private`coef, 3] - 
           ODEsTransformations`Private`\[Lambda] 
           ODEsTransformations`Private`aux1; 
         ODEsTransformations`LiouvilleNormalForm[
           Part[ODEsTransformations`Private`coef, 1], 
           Part[ODEsTransformations`Private`coef, 2], 
           ODEsTransformations`Private`aux2, ODEsTransformations`Private`aux1,
            ODEsTransformations`Private`x, 
